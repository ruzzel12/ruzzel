#include <iostream>
using namespace std;

class GradeBook{
public:
	void showMessage(string courseName){
		cout<<"Welcome to grade book: "<<courseName<<endl;
	}
};

int main(){
	string nameOfCourse;
	GradeBook myGradeBookObj; //create a GradeBook ojbect named myGradeBookObj
	cout<<"Enter course name: ";
	getline(cin,nameOfCourse);
	
	cout<<endl;
	
	myGradeBookObj.showMessage(nameOfCourse) ;
}
